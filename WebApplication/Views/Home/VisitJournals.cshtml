@model List<BusinessLogic.Models.VisitJournal>;
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Посещаемость</h1>
    <table class="table mt-4">
  <thead class="thead-dark bg-primary text-light">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Студент</th>
      <th scope="col">Лекция</th>
      <th scope="col">Оценка</th>
      
    </tr>
  </thead>
  <tbody>
      @foreach(var vj in Model){
       <tr>
      <th scope="row">@vj.Id</th>
      <td>@vj.Student.Name @vj.Student.Surname</td>
      <td>@vj.Lection.Subject</td>
      <td>@vj.Mark</td>
      
    </tr>
      }
  </tbody>
</table>
  <div class="band-list text-light">
    <a class="btn btn-outline-primary" asp-action="Index">Вернуться на главную</a>
 </div>
</div>
