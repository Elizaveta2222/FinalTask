@model List<BusinessLogic.Models.Teacher>;
@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">Певцы</h1>
    <table class="table mt-4">
  <thead class="thead-dark bg-primary text-light">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Имя</th>
      <th scope="col">Страна</th>
      <th scope="col">Жанр</th>
      <th scope="col">Песни</th>
      <th scope="col">Управление</th>
    </tr>
  </thead>
  <tbody>
      @foreach(var singer in Model){
       <tr>
      <th scope="row">@singer.Id</th>
      <td>@singer.Name</td>
      <td>@singer.Surname</td>
    </tr>
      }
  </tbody>
</table>

<div class="band-list text-light">
    <a class="btn btn-outline-primary" asp-action="CreateTeacher">Добавить нового певца</a>
 </div>

</div>
